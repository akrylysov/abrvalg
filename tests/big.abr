y(1, 2)

# Conditions
# ----------
if 1 + 2 == 3:
    print(1)

if 1 + 2 != 3:
    print(2)
else:
    print(1)

if 1:
    print(1)
elif 2:
    print(2)
elif 3:
    print(3)
else:
    print(10)

match y(1, 2):
    when 0:
        print(1)
    when 2:
        print(666)
    else:
        print('11')

# Loops
# -----
while 1:
    print(123)
    break

x = 10
while x > 0:
    x = x - 1
    if x % 2:
        print(x)
    else:
        continue

for x in [1]:
    print(x)

for x in 1...10:
    print(x)

